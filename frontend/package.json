{
   "name": "react-router-auth-template",
   "private": true,
   "license": "AGPL-3.0",
   "author": "Web Solutions and Client Access (WSCA)",
   "scripts": {
      "preinstall": "npx only-allow pnpm",
      "build": "pnpm -F auth-demo run build",
      "build:modules": "pnpm -F './modules/*' run build",
      "build:app": "pnpm -F auth-demo run build",
      "dev": "pnpm -F auth-demo run dev",
      "dev:app": "pnpm -F auth-demo run dev",
      "test": "pnpm -r run test",
      "lint": "pnpm -r run lint",
      "tsc": "pnpm -r run tsc",
      "clean": "pnpm -r run clean",
      "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\""
   },
   "devDependencies": {
      "prettier": "^3.2.5",
      "@trivago/prettier-plugin-sort-imports": "^4.3.0",
      "prettier-plugin-tailwindcss": "^0.5.12"
   },
   "engines": {
      "node": ">=22.0.0",
      "pnpm": ">=10.6.2"
   },
   "packageManager": "pnpm@10.6.2"
}
